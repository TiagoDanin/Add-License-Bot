{
	"name": "add-license-bot",
	"main": "index.js",
	"preferGlobal": false,
	"version": "1.3.5",
	"description": "Generate LICENSE file from package.json",
	"author": {
		"email": "TiagoDanin@outlook.com",
		"name": "Tiago Danin",
		"url": "https://TiagoDanin.github.io"
	},
	"license": "MIT",
	"keywords": [
		"github-app",
		"github-bot",
		"license",
		"probot",
		"probot-app"
	],
	"scripts": {
		"start": "probot run ./index.js",
		"dev": "nodemon",
		"test": "xo && jest",
		"test:watch": "jest --watch --notify --notifyMode=change --coverage"
	},
	"engines": {
		"node": ">= v14.16.1"
	},
	"private": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TiagoDanin/Add-License-Bot.git"
	},
	"homepage": "http://TiagoDanin.github.io/Add-License-Bot",
	"bugs": {
		"url": "https://github.com/TiagoDanin/Add-License-Bot/issues"
	},
	"github": {
		"name": "Add-License-Bot",
		"owner": "TiagoDanin"
	},
	"files": [
		"LICENSE",
		"Procfile",
		"README.md",
		"app.yml",
		"index.js",
		"package.json"
	],
	"dependencies": {
		"choosealicense-list": "^1.2.0",
		"probot": "12.3.3"
	},
	"devDependencies": {
		"jest": "26.6.3",
		"nodemon": "^2.0.7",
		"smee-client": "1.2.2",
		"xo": "^0.39.1"
	},
	"xo": {
		"rules": {
			"no-await-in-loop": "warn",
			"promise/prefer-await-to-then": "warn"
		},
		"semicolon": false
	},
	"jest": {
		"testEnvironment": "node"
	},
	"nodemonConfig": {
		"exec": "npm start",
		"watch": [
			".env",
			"."
		]
	}
}
